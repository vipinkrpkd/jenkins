pipeline { 
agent any 
    stages {  
        stage ('Build') { 
		steps {
			sh 'mvn install'
		}
        }
        stage ('Test') { 
		steps{ echo "Test done" }
        
        }
       
        stage ('Deploy') { 
        	steps{ echo "Deploy done "}
        }

	stage ('Publish'){
		steps{ sh 'mvnw package' }
		post {
			success { archiveArtifacts 'target/*.jar'}
		}
	}
		
    }           
 }

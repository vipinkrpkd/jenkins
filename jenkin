pipeline{
    agent any
    tools { 
      maven 'M2_HOME' 
      jdk 'JAVA_HOME' 
    stages{
        stage("fetch code"){
            steps {
                git branch: 'dev', url: 'https://github.com/vipinkrpkd/jenkins.git'
            }
        }
        stage('Build'){
            steps {
                sh 'mvn install'
            }
        }
        stage('Test'){
            steps {
                sh 'mvn test'
            }
        }
    }
}
